#include<stdio.h>
  int main()
{
  int arr[100];
  int size,i,duplicate,sum=0;
  scanf("%d",&size);
  for(i=0;i<size;i++)
  {
    scanf("%d",&arr[i]);
  }
  duplicate=arr[0];
  for(i=0;i<size;i++)
  {
    if(duplicate==arr[i])
    {
      sum+=duplicate;
      duplicate++;
    }
  }
  printf("%d",sum);
  return 0;
}
  
